<div class="container">
  <div class="content">
    <table class="table">
      <tr>
        <th>ID</th>
        <th>Full Name</th>
        <th>Email</th>
        <th>Action</th>
      </tr>
      <% @users.each do |u| %>
        <tr>
          <td><%= u.id%></td>
          <td><%= u.full_name %></td>
          <td><%= u.email %></td>
          <td>action</td>
        </tr>
      <% end -%>
    </table>
  </div>
</div>